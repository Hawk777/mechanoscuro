[gd_scene load_steps=5 format=2]

[ext_resource path="res://main_menu/background.png" type="Texture" id=1]
[ext_resource path="res://main_menu/mute_button.tscn" type="PackedScene" id=2]
[ext_resource path="res://levels/01/level.tscn" type="PackedScene" id=3]

[sub_resource type="GDScript" id=1]
script/source = "extends Node

export(PackedScene) var first_level: PackedScene


func _on_play() -> void:
	get_tree().change_scene_to(first_level)
"

[node name="Root" type="CanvasLayer"]
script = SubResource( 1 )
first_level = ExtResource( 3 )

[node name="Background" type="Sprite" parent="."]
texture = ExtResource( 1 )
centered = false

[node name="PlayButton" type="Button" parent="."]
anchor_left = 0.5
anchor_top = 0.5
anchor_right = 0.5
anchor_bottom = 0.5
margin_left = -19.0
margin_top = -10.0
margin_right = 19.0
margin_bottom = 10.0
grow_horizontal = 2
grow_vertical = 2
text = "Play"
__meta__ = {
"_edit_use_anchors_": false
}

[node name="MuteButton" parent="." instance=ExtResource( 2 )]

[connection signal="pressed" from="PlayButton" to="." method="_on_play"]
