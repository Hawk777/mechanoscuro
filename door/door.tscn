[sub_resource type="GDScript" id=8]
script/source = "extends AnimatedSprite

export(bool) var is_open: bool


func _ready() -> void:
	if is_open:
		self.animation = \"close\"


func open() -> void:
	is_open = true
	self.play(\"open\")


func close() -> void:
	is_open = false
	self.play(\"close\")


func toggle() -> void:
	if is_open:
		close()
	else:
		open()
"

[node name="AnimatedSprite" type="AnimatedSprite"]
frames = SubResource( 7 )
